<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.intervaltrainer"
    android:versionCode="5"
    android:versionName="1.4.1" >

    <uses-sdk
        android:minSdkVersion="9"
        android:targetSdkVersion="20" />

    <application
        android:name="com.intervaltrainer.InterApp"
        android:allowBackup="true"
        android:icon="@drawable/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <activity
            android:name="com.intervaltrainer.HomeScreen"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name="com.intervaltrainer.QuizActivity"
            android:label="@string/app_name" >
        </activity>
        <activity
            android:name="com.intervaltrainer.Options"
            android:label="@string/app_name"
            android:theme="@style/AppTheme" >
        </activity>
        <activity
            android:name="com.intervaltrainer.FlashcardListActivity"
            android:label="@string/app_name"
            android:theme="@style/AppTheme" >
        </activity>
        <activity
            android:name="com.intervaltrainer.FlashcardPagerActivity"
            android:label="@string/app_name"
            android:parentActivityName=".FlashcardListActivity"
            android:theme="@style/AppTheme" >
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="com.slamnig.flashcards.FlashcardListActivity" />
        </activity>
    </application>

</manifest>